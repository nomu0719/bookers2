 <h2>Users</h2>
 
 <header>
  <div><h6>image</h6></div>
  <div><h6>name</h6></div>
  <div></div>
 </header>
<% @users.each do |user| %>
<div>
  <P><%= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %></P>
  <p><%= user.name %></p>
  <p><%= link_to "Show", user_path(user.id) %> </p>
<% end %>
</div>
